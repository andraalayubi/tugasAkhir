  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro%3A400%2C500%2C600" />
  <link rel="stylesheet" href="/css/admin.css" />
  <link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css">
</head>
<body>
  <div class="group-1000002749-1Am">
    <div class="background-REV">
      <div class="auto-group-gux5-PU9">
        <div class="group-1000002759-Qu3">
          <div class="admin-maskerku-Xim">ADMIN MASKERKU</div>
          <img class="vector-6-NDb" src="REPLACE_IMAGE:1:106" />
          <div class="group-1000002754-uUR">
            <div class="group-1000002755-pbP">
              <img class="auto-group-58r9-iih" src="./assets/auto-group-58r9.png" />
              <div class="dashboard-TAV">Dashboard</div>
            </div>
            <div class="auto-group-ws17-6DT">
              <div class="group-1000002757-Yjf" data-bs-toggle="modal" data-bs-target="#exampleModal">
                <img class="auto-group-yaw7-5zV" src="./assets/auto-group-yaw7.png" />
                <div class="add-stock-E6h">Add Stock</div>
              </div>

              <!-- Modal -->
              <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel"
                aria-hidden="true">
                <div class="modal-dialog modal-sm">
                  <div class="modal-content">
                    <div class="modal-header">
                        <h1 class="modal-title fs-5" id="exampleModalLabel">Tambah Stok Produk</h1>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">
                        <form action="/admin">
                            <select class="form-select mb-3" aria-label="Default select example">
                                <option selected>Pilih Produk</option>
                                <option value="1">Masker Headloop</option>
                                <option value="2">Masker Earloop</option>
                            </select>
                            <div class="form-floating">
                                <input type="number" class="form-control" id="jumlah" placeholder="Jumlah">
                                <label>Jumlah</label>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                            <button type="submit" class="btn btn-primary">Save changes</button>
                        </div>
                    </form>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="auto-group-glzh-MBK">
          <div class="breadcrumb-tww">
            <p class="dashboard-4Ld">Dashboard</p>
            <div class="dropdown">
              <button class="border-0" data-bs-toggle="dropdown" aria-expanded="false">
                <img class="menu-Ns7" src="./assets/menu.png" />
              </button>
              <ul class="dropdown-menu">
                <li><a class="dropdown-item fs-5" href="/logout">Sign Out</a></li>
              </ul>
            </div>
          </div>
          <div class="auto-group-vfbt-WCd">
            <div class="group-1000002760-epd">
              <% dataAdmin.cartData.products.forEach(item=> { %>
              <div class="todays-money-nR3">
                <div class="text-uVf">
                  <p class="todays-money-Tn5">Produk ID: <%= item.product_id %></p>
                  <p class="item-53000-YoX">Stok: <%= item.stock_quantity %></p>
                </div>
                <img class="icon-HFK" src="./assets/icon-QDb.png" />
              </div>
              <% }); %>
            </div>
            <div class="background-mTK">
              <div class="text-6Vb">
                <p class="recent-sales-rDs">
                  <span class="recent-sales-rDs-sub-0 text-black">&nbsp;Recent&nbsp;</span>
                  <span class="recent-sales-rDs-sub-1 text-black"> Sales</span>
                </p>
              </div>
              <table class="table table-hover">
                <thead>
                    <tr>
                      <th scope="col" class="w-25 fs-4 text-secondary">Customer Name</th>
                      <th scope="col" class="w-25 fs-4 text-secondary">Date</th>
                      <th scope="col" class="w-10 fs-4 text-secondary">Product ID</th>
                      <th scope="col" class="w-10 fs-4 text-secondary">Quantity</th>
                      <th scope="col" class="w-15 fs-4 text-secondary me-3">Total</th>
                    </tr>
                </thead>
                <tbody>
                    <% dataAdmin.cartData.latestSales.forEach(item => { %>
                        <tr>
                            <td class="fs-4 fw-bolder"><%= item.user.username %></td>
                            <td class="fs-4 fw-bolder"><%= item.date.toISOString().split('T')[0] %></td>
                            <td class="fs-4 fw-bolder"><%= item.product_id %></td>
                            <td class="fs-4 fw-bolder"><%= item.quantity %></td>
                            <td class="fs-4 fw-bolder">Rp. <%= item.total %>.000,-</td>
                        </tr>
                    <% }); %>
                </tbody>
            </table>
            </div>
          </div>
        </div>
      </div>
      <div class="auto-group-ghdf-KmX">
        <div class="rectangle-7-rFf">
        </div>
        <div class="total-revenue-mdX">
          <div class="group-1000002743-VpR">
            <p class="total-revenue-3b3">Total Revenue</p>
            <div class="row">
              <!-- Produk 1 -->
              <div class="col">
                  <h2>Produk 1</h2>
                  <table class="table table-hover">
                      <thead>
                          <tr>
                              <th scope="col" class="w-15 fs-4 text-secondary">Tanggal</th>
                              <th scope="col" class="w-15 fs-4 text-secondary">Total</th>
                          </tr>
                      </thead>
                      <tbody>
                          <!-- Isi tabel produk 1 -->
                          <% dataAdmin.produk1.forEach(item => { %>
                              <tr>
                                  <td class="fs-4 fw-bolder"><%= item.date.toISOString().split('T')[0] %></td>
                                  <td class="fs-4 fw-bolder"><%= item._sum.total %></td>
                              </tr>
                          <% }); %>
                      </tbody>
                  </table>
              </div>
      
              <!-- Produk 2 -->
              <div class="col">
