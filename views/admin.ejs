                              <th scope="col" class="w-15 fs-4 text-secondary">Total</th>
                          </tr>
                      </thead>
                      <tbody>
                          <!-- Isi tabel produk 2 -->
                          <% dataAdmin.produk2.forEach(item => { %>
                              <tr>
                                  <td class="fs-4 fw-bolder"><%= item.date.toISOString().split('T')[0] %></td>
                                  <td class="fs-4 fw-bolder"><%= item._sum.total %></td>
                              </tr>
                          <% }); %>
                      </tbody>
